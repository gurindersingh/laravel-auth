{
  "name": "gurinder/laravel-auth",
  "description": "Laravel Authentication System - Including UI",
  "keywords": [
    "laravel",
    "authenticate",
    "Socialite"
  ],
  "homepage": "https://github.com/gurindersingh/laravel-auth",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gurinder Chauhan",
      "email": "gurinderchauhan@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/auth": "^5.6",
    "illuminate/container": "^5.6",
    "illuminate/contracts": "^5.6",
    "illuminate/database": "^5.6",
    "illuminate/events": "^5.6",
    "illuminate/pagination": "^5.6",
    "illuminate/routing": "^5.6",
    "illuminate/validation": "^5.6",
    "illuminate/view": "^5.6",
    "laravel/socialite": "^3.0",
    "google/apiclient": "^2.2",
    "facebook/graph-sdk": "^5.6",
    "abraham/twitteroauth": "^0.7.4",
    "illuminate/cache": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Gurinder\\LaravelAuth\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Gurinder\\LaravelAuth\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gurinder\\LaravelAuth\\AuthServiceProvider"
      ]
    }
  }
}
